{
  "name": "nxiv",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.2.1",
  "description": "Noumenon, the typescript library for XIVAPI",
  "author": "Marius Andresen",
  "license": "MIT",
  "homepage": "https://noumenon.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/kalosar/noumenon"
  },
  "bugs": "https://github.com/kalosar/noumenon/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "clean": "rimraf dist",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "bump": "bumpp",
    "release": "npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.6.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.20.0",
    "esno": "^0.16.3",
    "pnpm": "^7.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "vite": "^3.0.4",
    "vitest": "^0.19.1"
  }
}
